{
  "version": 2,
  "functions": {
    "api/index.php": { "runtime": "vercel-php@0.7.0" }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/(css|js|images|assets)/(.*)", "dest": "/public/$1/$2" },
    { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ]
}